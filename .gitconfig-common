#[includeif "gitdir:~/src/rajkipa/"]
#        path = .gitconfig-personal

[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  unstage = reset HEAD --
  last = log -1 HEAD
  visual = !gitk

[status]
  submodulesSummary = true
  submoduleSummary = true

